{
  "name": "auto-branch-pr",
  "version": "1.0.0",
  "private": true,
  "description": "Auto create Pull Request to a base branch when a destination changes",
  "author": "Tonye Jack <jtonye@ymail.com>",
  "license": "ISC",
  "repository": "https://github.com/jackton1/auto-branch-pr.git",
  "homepage": "https://github.com/jackton1/auto-branch-pr",
  "bugs": "https://github.com/jackton1/auto-branch-pr/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "12.3.3"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/nock": "11.1.0",
    "@types/node": "13.13.12",
    "@typescript-eslint/parser": "2.34.0",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "24.9.0",
    "nock": "12.0.3",
    "nodemon": "2.0.4",
    "smee-client": "1.1.0",
    "standard": "14.3.4",
    "ts-jest": "25.5.1",
    "typescript": "3.9.5"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
